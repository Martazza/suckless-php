<?xml version="1.0" encoding="utf-8" ?>
<phpdox xmlns="http://xml.phpdox.net/config">
	<project name="Example" source="${basedir}/" workdir="${basedir}/build/html">
		<collector backend="parser" />
		<generator output="${basedir}/build/html">
			<build engine="html" output="html"/>
			<file extension="html" />
			<include mask="*.php" />
			<exclude mask="/examples" />
		</generator>
	</project>
</phpdox>

